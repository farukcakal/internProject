@model List<JobsViewModel>

    @foreach (JobsViewModel jobsVm in Model)
    {
    <div class="card" style="margin-bottom:10px;">
        <div class="card-body">
            <div class="container">
                <div class="row">

                    <div class="col-md-10">
                        <h4>@jobsVm.jobTitle</h4>
                    </div>

                    <div class="col-md-2">
                        <a href="/jobdetail/index/@jobsVm.jobSlug" class="btn btn-danger">İlanı İncele</a>
                    </div>

                </div>
            </div>
            
        </div>
    </div>
    
    }
